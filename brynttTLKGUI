98", 	PremiumOnly = false }) Tab:AddButton({ 	Name = "Zeouron(My Friend's Gui)", 	Callback = function() loadstring(game:HttpGet('https://raw.githubusercontent.com/voidy434343/Zeouron-Mainiatic/main/Tlk%20prison%20obf.txt'))() 		print("button pressed") 	end }) Tab:AddButton({ 	Name = "Red Gui", 	Callback = function() loadstring(game:HttpGet("https://raw.githubusercontent.com/bryntt-hub/Red-Gui/main/Red"))() 		print("button pressed") 	end }) Tab:AddButton({
